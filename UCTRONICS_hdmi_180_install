sudo cp ./uctronics_hdmi_180/kernel.img /boot/
sudo cp ./uctronics_hdmi_180/kernel7.img /boot/

sudo cp ./uctronics_hdmi_180/config.txt /boot/config.txt
sudo cp ./uctronics_hdmi_180/*.dtb /boot/
sudo cp ./uctronics_hdmi_180/overlays/*.dtb* /boot/overlays/
sudo cp -rf ./uctronics_hdmi_180/lib/* /lib/

sudo apt-mark hold raspberrypi-kernel
sudo reboot
